{
  "name": "@tvenceslau/injectable-decorators",
  "version": "1.0.1",
  "description": "injectable decorators extension for decorator validation",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "scripts": {
    "build": "rimraf ./lib && rimraf ./dist && gulp dev",
    "build:prod": "rimraf ./lib && rimraf ./dist && gulp prod",
    "test": "jest --coverage --testPathPattern=\"/tests/(unit|web)\" --globalSetup=\"\"",
    "test:all": "jest --coverage --testPathPattern=\"/tests/(unit|web|integration)\" --passWithNoTests",
    "prepare-release": "npm run build:prod && npm run test && npm run docs",
    "release": "./bin/tag-release.sh",
    "clean-publish": "npx clean-publish",
    "coverage": "npm run test:all && jest-coverage-badges --input \"./workdocs/coverage/coverage-summary.json\" --output \"./workdocs/badges\"",
    "docs": "npx rimraf ./docs && mkdir docs && npm install better-docs taffydb && gulp docs; npm remove better-docs taffydb"
  },
  "files": [
    "dist/",
    "lib/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TiagoVenceslau/injectable-decorators.git"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "typescript",
    "decorators",
    "database",
    "create",
    "transactions",
    "update",
    "read",
    "delete",
    "CRUD",
    "id",
    "timestamp"
  ],
  "author": "Tiago Venceslau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TiagoVenceslau/injectable-decorators/issues"
  },
  "homepage": "https://github.com/TiagoVenceslau/injectable-decorators#readme",
  "devDependencies": {
    "@tvenceslau/decorator-validation": "^2.0.5",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "gulp": "^4.0.2",
    "gulp-if": "^3.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-run-command": "^0.0.10",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "jest": "^29.7.0",
    "jest-coverage-badges": "^1.1.2",
    "jest-junit": "^16.0.0",
    "jsdoc": "^4.0.2",
    "jsdoc-mermaid": "^1.0.0",
    "markdown-include": "^0.4.3",
    "merge-stream": "^2.0.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vinyl-named": "^1.1.0",
    "webpack-stream": "^7.0.0"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.1"
  }
}
